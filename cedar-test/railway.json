{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "cd cedar-test && pip install -r requirements.txt"
  },
  "deploy": {
    "startCommand": "cd cedar-test && python web_server_fastapi.py",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}